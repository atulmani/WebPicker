{
  "hosting": {
  "public": "public",
  "rewrites": [
    {
      "source": "/propdial/**",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/propdial/**",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache"
        }
      ]
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false,
  "ignore": [
    "firebase.json",
    "**/.*",
    "**/node_modules/**"
  ]
}

}
